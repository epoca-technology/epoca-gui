{
    "name": "gui-development",
    "version": "1.4.0",
    "description": "Epoca is a pattern finding technology designed to trade Futures Contracts through Bitcoin Exchanges.",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "serve-development": "node ./node_modules/gulp/bin/gulp.js preBuildDev && node ./node_modules/@angular/cli/bin/ng serve --configuration development",
        "serve-production": "node ./node_modules/gulp/bin/gulp.js preBuildProd && node ./node_modules/@angular/cli/bin/ng serve --configuration production",
        "build-development": "node ./node_modules/gulp/bin/gulp.js preBuildDev && node ./node_modules/@angular/cli/bin/ng build --configuration development && node ./node_modules/gulp/bin/gulp.js postBuildDev",
        "build-production": "node ./node_modules/gulp/bin/gulp.js preBuildProd && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --configuration production && node ./node_modules/gulp/bin/gulp.js postBuildProd"
    },
    "private": true,
